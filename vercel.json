{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/(.*).(js|css|map|json|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot)$",
      "dest": "/$1.$2"
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ]
}
