{
  "version": 2,
  "builds": [
    {
      "src": "Inventory-Management-System-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/Inventory-Management-System-frontend/index.html" }
  ]
}
